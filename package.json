{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "prisma:generate": "npx dotenv-cli -e .env -- prisma generate",
    "prisma:push": "npx dotenv-cli -e .env -- prisma db push",
    "prisma:studio": "npx dotenv-cli -e .env -- prisma studio",
    "prisma:migrate": "npx dotenv-cli -e .env -- prisma migrate dev",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/supabase": "^2.0.3",
    "@prisma/client": "^6.7.0",
    "@supabase/gotrue-js": "^2.89.0",
    "@supabase/supabase-js": "^2.89.0",
    "joi": "^18.0.2",
    "nuxt": "^3.0.0",
    "prisma": "^6.7.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@vueuse/nuxt": "^14.1.0",
    "dotenv-cli": "^11.0.0",
    "minimatch": "^3.1.2"
  }
}
